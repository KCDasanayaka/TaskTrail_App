{
  "expo": {
    "name": "ToDo",
    "slug": "todo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ToDo 2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/ToDo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.todo", 
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library."
      }
    },
    "android": {
      "permissions": [
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "NOTIFICATION" 
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ToDo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/To-Do1.png"
    },
    "plugins": [
      "expo-router" 
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
